<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>E-Demo</display-name>
  <welcome-file-list>
    <welcome-file>home.html</welcome-file>
    </welcome-file-list>
    <servlet-mapping>
    <servlet-name>signup</servlet-name>
    <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet>
    <servlet-name>signup</servlet-name>
    <servlet-class>com.org.User_SignupServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.org.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    <servlet-name>insert</servlet-name>
    <url-pattern>/insert_servlet</url-pattern>
    </servlet-mapping>
    <servlet>
    <servlet-name>insert</servlet-name>
    <servlet-class>com.org.Insert_Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
    <servlet-name>DeleteProduct</servlet-name>
    <url-pattern>/ds</url-pattern>
    </servlet-mapping>
    <servlet>
    <servlet-name>DeleteProduct</servlet-name>
    <servlet-class>com.org.DeleteProduct</servlet-class>
    </servlet>
    
    <servlet-mapping>
    <servlet-name>showallServlet</servlet-name>
    <url-pattern>/showall</url-pattern>
    </servlet-mapping>
    <servlet>
    <servlet-name>showallServlet</servlet-name>
    <servlet-class>com.org.fatch_All_Prodect</servlet-class>
    </servlet>
    
    
</web-app>